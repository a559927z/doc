public class UuidUtil {
    /**
     * 返回一个32位大写的UUID
     * @author guanjian
     * @return
     */
    public static String uuid32(){
        String uuid = UUID.randomUUID().toString().replaceAll("-", "");
        return uuid.toUpperCase();
    }
    
    public static void main(String[] args) {
        System.out.println( uuid32());
    }
}
